[
  VAR monvecteur (vec int);
  SET monvecteur (alloc 50);

  VAR i int; 
  SET i 0; 

  WHILE (lt i (len monvecteur)) [ 
        SET (nth monvecteur i) i; 
        SET i (add i 1) 
    ]; 

  SET i 0;

  WHILE (lt i (len monvecteur)) [ 
        ECHO (nth monvecteur i);
        SET i (add i 1) 
    ]
]