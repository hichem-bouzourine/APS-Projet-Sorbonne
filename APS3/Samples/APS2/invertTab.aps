[
    CONST arr (vec int) (alloc 5);
    SET (nth arr 0) 10;
    SET (nth arr 1) 20;
    SET (nth arr 2) 30;
    SET (nth arr 3) 40;
    SET (nth arr 4) 50;
    VAR i int;
    SET i 0;
    WHILE (lt i (div (len arr) 2)) 
        [
            CONST temp int (nth arr i);
            SET (nth arr i) (nth arr (sub (len arr) (add i 1)));
            SET (nth arr (sub (sub (len arr) i) 1)) temp;
            SET i (add i 1)
        ];
    ECHO (nth arr 0)
]